#format: sh
# 
# Configuration file (mgiconfig) for mgigff product.
# See also: config.cfg in this directory.
#
if [ "${MGICONFIG}" = "" ]
then
    MGICONFIG=/usr/local/mgi/live/mgiconfig
    export MGICONFIG
fi
. ${MGICONFIG}/master.config.sh

DATADIR=${DATALOADSOUTPUT}/mgi/mgigff3/downloads
export DATADIR

WORKINGDIR=${DATALOADSOUTPUT}/mgi/mgigff3/work
export WORKINGDIR

DISTRIBDIR=${FTPROOT}/pub/mgigff3
export DISTRIBDIR
